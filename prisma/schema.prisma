generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["fullTextSearch"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Film {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  release     Int
  length      Int
  lastUpdate  DateTime
}
